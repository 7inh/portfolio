{
  "go.inferGopath": false,
  "cSpell.words": [
    "ampm",
    "clickaway",
    "Iconify",
    "Snackbars",
    "svgs",
    "testid",
    "uuidv",
    "vite"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "**/.git": true, // this is a default value
    "**/.DS_Store": true // this is a default value
  },
  "search.exclude": {
    "**/tmp": true,
    "**/.git": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/build": true,
    "**/.cache": true,
    "**/deps": true,
    "**/deps_temp": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/coverage": true,
    "**/build": true,
    "**/.cache": true,
    "**/deps": true,
    "**/deps_temp": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  }
}
